{
  "buildCommand": "pnpm build",
  "outputDirectory": "build/client",
  "framework": "remix",
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev:vercel",
  "functions": {
    "app/entry.vercel.tsx": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app/entry.vercel.tsx"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
